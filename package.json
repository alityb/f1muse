{
  "name": "f1muse-api",
  "version": "1.0.0",
  "description": "F1 analytics backend - deterministic query validation and execution",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run --exclude 'tests/integration/**' --exclude 'tests/contract/**' --exclude 'tests/cache/**'",
    "test:integration": "vitest run tests/integration/**/*.test.ts tests/contract/**/*.test.ts tests/cache/**/*.test.ts",
    "test:nl": "vitest run tests/nl/**/*.test.ts",
    "test:contract": "vitest run tests/contract/**/*.test.ts",
    "test:cache": "vitest run tests/cache/**/*.test.ts",
    "test:db": "createdb f1muse_test || true",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "validate:all": "npm run typecheck && npm run lint && npm run test",
    "validate:quick": "npm run typecheck && npm run test:unit && npm run test:nl",
    "validate:full": "npm run typecheck && npm run lint && npm run test:unit && npm run test:nl && npm run test:integration",
    "db:test:up": "docker compose -f docker-compose.test.yml up -d && sleep 3",
    "db:test:down": "docker compose -f docker-compose.test.yml down",
    "ingest:teammate-gap:2025": "tsx src/ingestion/teammate-gap-race-2025.ts",
    "ingest:teammate-gap:race:2025": "tsx src/ingestion/teammate-gap-race-2025.ts",
    "ingest:teammate-gap:qualifying:2025": "tsx src/ingestion/teammate-gap-qualifying-2025.ts",
    "ingest:season": "ts-node src/etl/ingest-season-2025.ts",
    "import:f1db:safe": "bash f1db-import/import-f1db-safe.sh",
    "etl:pace-metrics": "ts-node src/etl/populate-pace-metrics-2025.ts",
    "etl:laps:all": "./venv/bin/python src/etl/ingest-laps-2025.py",
    "etl:laps:round": "./venv/bin/python src/etl/ingest-laps-2025.py --round",
    "etl:laps:season": "./venv/bin/python src/etl/ingest-laps.py --season",
    "etl:laps:2022": "./venv/bin/python src/etl/ingest-laps.py --season 2022",
    "etl:laps:2023": "./venv/bin/python src/etl/ingest-laps.py --season 2023",
    "etl:laps:2024": "./venv/bin/python src/etl/ingest-laps.py --season 2024",
    "validate:teammate-gap": "tsx src/diagnostics/validate-teammate-gap-ingestion.ts",
    "diagnostics:teammate-gap": "tsx src/diagnostics/teammate-gap-coverage.ts",
    "ingest:matchup-matrix:2025": "tsx src/ingestion/matchup-matrix-2025.ts",
    "etl:qualifying:season": "./venv/bin/python src/etl/ingest-qualifying.py --season",
    "etl:qualifying:2022": "./venv/bin/python src/etl/ingest-qualifying.py --season 2022",
    "etl:qualifying:2023": "./venv/bin/python src/etl/ingest-qualifying.py --season 2023",
    "etl:qualifying:2024": "./venv/bin/python src/etl/ingest-qualifying.py --season 2024",
    "etl:qualifying:2025": "./venv/bin/python src/etl/ingest-qualifying.py --season 2025",
    "etl:qualifying:all": "npm run etl:qualifying:2022 && npm run etl:qualifying:2023 && npm run etl:qualifying:2024 && npm run etl:qualifying:2025"
  },
  "keywords": [
    "f1",
    "analytics",
    "fastf1"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-bedrock-runtime": "^3.980.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
